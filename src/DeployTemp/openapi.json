{
  "openapi": "3.0.1",
  "info": { "title": "External Signals API", "version": "1.3.0" },
  "servers": [ { "url": "http://localhost:7071/api" } ],
  "paths": {
    "/price/dayahead": {
      "get": {
        "operationId": "DayAheadPrice",
        "parameters": [ { "name": "zone", "in": "query", "required": true, "schema": { "type": "string" } }, { "name": "date", "in": "query", "required": false, "schema": { "type": "string", "format": "date" } } ],
        "responses": { "200": { "description": "OK" } }
      }
    },
    "/weather/hourly": {
      "get": {
        "operationId": "WeatherHourly",
        "parameters": [ { "name": "city", "in": "query", "required": true, "schema": { "type": "string" } }, { "name": "date", "in": "query", "required": false, "schema": { "type": "string", "format": "date" } } ],
        "responses": { "200": { "description": "OK" } }
      }
    }
  }
}